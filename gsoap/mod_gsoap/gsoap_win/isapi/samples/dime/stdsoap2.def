	soap_init	PRIVATE
	soap_serve  PRIVATE
	soap_delete PRIVATE
	soap_done	PRIVATE
	soap_end	PRIVATE
	soap_register_plugin_arg PRIVATE
	soap_lookup_plugin PRIVATE
	
	mod_gsoap_init PRIVATE      ; initializer called before loading
	mod_gsoap_terminate PRIVATE ; called before uploading 