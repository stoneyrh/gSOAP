prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: GSoap
Description: SOAP C Web Services with SSL, ZLIB, DOM
Version: 2.8.100
Requires:
Libs: -L${libdir} -lgsoapssl
Libs.private: -L/usr/local/opt/openssl/lib -lssl -lcrypto -lz -lm
Cflags:  -I/usr/local/opt/openssl/include -DWITH_OPENSSL -DWITH_GZIP -DWITH_DOM -I${includedir}
